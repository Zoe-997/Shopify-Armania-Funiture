@charset "UTF-8";
/*FOOTER STYLE*/
.footer {
    clear: both;
    padding-top: 0px;
}

.footer .main-footer{
    padding-top: 70px;
    padding-bottom: 70px;
}

.footer .main-footer .widget.show-border{
    border-width: 0 1px 0 0;
    border-style: solid;
    height: 100%;
}
.rtl .footer .main-footer .widget.show-border{
    border-width: 0 0 0 1px;
}
.footer .footer-top .widget_footer .content,
.footer .footer-top .widget_footer .content_html{
    font-size:14px;
    margin-bottom:15px;
}

.block-subscribe .section-title-group{
    margin-bottom: 30px;
    text-align: initial;
    color: var(--color-main);
    color: #adadad;
}

.block-subscribe .section-title-group .section-content{
    font-size: 16px;
    line-height: 20px;
    margin-top: 20px;
}

.block-subscribe .section-title-group .section-title{
    font-size: 22px;
    line-height: 25px;
    color: #ffffff;
}

.footer .main-footer .widget-title {
    -webkit-box-flex: 1;
    -ms-flex: 1 1 auto;
    flex: 1 1 auto;
    margin-top: 0;
    margin-bottom: 0;
    font-weight: 500;
    padding: 0 0 25px 0;
    letter-spacing: 0;
    line-height: 1;
    text-transform: capitalize;
    font-size: 22px;
    line-height: 25px;
}

.footer .main-footer .widget-title .icon_mobile{
    display:none;
}

@media (max-width: 1024px) {
    .footer {
        padding-bottom: 50px;
    }
}

.footer .main-footer {
    background-color: transparent;
    background-repeat: no-repeat;
    background-size: cover;
    background-attachment: scroll;
    background-position: center center;
    font-size: 14px;
}
.footer .main-footer .container > .row{
	justify-content: space-between;
}
.footer .main-footer .list-payment li {
    display: inline-block;
    list-style: none;
    margin: 0px 7.5px;
}
.footer .main-footer .list-payment li:first-child {
    margin-left: 0px;
}
.footer .main-footer .widget_logo  .footer-logo-wrap{
    margin-bottom:30px;
}
.footer-contact-items .footer-contact-item{
    margin-bottom: 10px;
}
.footer-contact-items .footer-contact-item:last-child{
    margin-bottom: 0;
}
.footer .social .laber-social-link .list-socials{
    display: flex;
    flex-wrap: wrap;
    margin-top: 20px;
}
.footer .social .laber-social-link .list-socials li{
    display: inline-block;
    margin-right: 15px;
    margin-left: 0;
}
.footer .social .laber-social-link .list-socials li:last-child{
    margin-right: 0;
}
.footer .social.footer-style .laber-social-link .list-socials li{
    order: 10;
    text-align: center;
}
.footer .social.footer-style .laber-social-link .list-socials li a{
    line-height: 20px;
    width: 20px;
    height: 20px;
    font-size: 20px;
}

.footer .text-end .social .laber-social-link .list-socials{
    justify-content: flex-end;
}

.footer-mail .signup-newsletter-form {
    position: relative;
    margin-top: 20px;
}

.footer-mail .signup-newsletter-form .actions-wrapper{
    position: static;
}

.footer-mail .signup-newsletter-form input{
    width: 100%;
    height: 61px;
    line-height: 59px;
    border-radius: var(--border-radius);
    outline: none;
    padding: 0 105px 0 28px;
    border-width: 1px;
    border-style: solid;
    font-size: 18px;
}

.footer-mail .signup-newsletter-form input:placeholder{
    font-size: 20px;
}

.footer-mail .signup-newsletter-form .submit-btn{
    position: absolute;
    top: 2px;
    right: 2px;
    bottom: 2px;
    z-index: 1;
    border-width: 0;
    border-radius: 0 var(--border-radius) var(--border-radius) 0;
    padding: 0 38px;
    background-color: transparent;
}

.footer-mail .signup-newsletter-form .submit-btn svg{
    width: 29px;
}

.footer .main-footer .menu_footer .menu {
    list-style: none;
    padding-left:10px
}

.footer .main-footer .menu_footer .menu a {
    position: relative;
    font-size: 16px;
    line-height:18px;
}

.footer .main-footer .menu_footer .menu li.menu-item {
    line-height: 1;
    margin-bottom: 17px;
}

.footer .main-footer .menu_footer .menu li.menu-item:last-child{
    margin-bottom:0;
}

/* Footer featured Products */
.footer .product_list_widget .product-item{
    padding-bottom: 20px;
}
/* End footer featured Products */

.footer .footer-note{
    padding-top: 40px;
    padding-bottom: 40px;
    border-top: 1px solid #2e2e2e;
}

.footer .footer-note.show-border{
    padding-bottom: 13px;
}

.footer .footer-note .custom-html-link{text-align: center;}
.footer .footer-note .custom-html-link p{line-height:1;margin:0;}
.footer .footer-note .custom-html-link a{
	color:#999;
    padding:0 6px;
    display:inline-block;
    margin-bottom:13px;
}
.footer .footer-note .custom-html-link a:hover{color:var(--color-link-hover);}
.footer-note .extra-info .inner-extra-info{
    display: flex;
    flex-wrap: wrap;
/*     align-items: baseline; */
}

.footer-note .extra-info .inner-extra-info .social{
    margin-right: 40px;
}

.footer-note .extra-info .inner-extra-info .social .title-socials{
    display: none;
}

.footer-note .extra-info .inner-extra-info .social .laber-social-link .list-socials li{
    margin-right: 38px;
}

.footer-note .extra-info .inner-extra-info .social .laber-social-link .list-socials li:last-child{
    margin-right: 0;
}

.footer-note .extra-info .inner-extra-info .social .laber-social-link .list-socials li a{
    width: auto;
    height: auto;
    border-width: 0;
}

.footer .footer-note .coppy-right p{
    margin: 0;
}
.footer .footer-note .social .laber-social-link .list-socials{
/*     margin-top: 0; */
}
.footer .parent-list .item-list{
    min-height: auto;
}
.footer .footer-note .parent-list .item-list a{
    color: var(--color-text-body);
}
.footer .footer-note .parent-list .item-list a:hover,
.footer .footer-note .parent-list .item-list li.active a{
    color: var(--color-link-hover);
}
.footer .parent-list .child-list{
    top: auto;
    bottom: calc(100% + 20px);
    transform: translateX(-50%);
    left: 50%;
    box-shadow: 0 20px 30px rgba(0, 0, 0, .12);
}
.footer .parent-list .item-list:hover .child-list{
    visibility: visible;
    opacity: 1;
    bottom: 100%;
    top: auto;
    transform: translateX(-50%);
}
.parent-list .item-list>a{
    line-height: initial;
}
.footer .footer-note .footer_paypal,
.footer .footer-note .coppy-right {
    width: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
}
.footer .footer-note .text-start .inner-extra-info,
.footer .footer-note .text-start .footer_paypal,
.footer .footer-note .text-start .coppy-right{
    justify-content: flex-start;
}
.footer .footer-note .text-end .inner-extra-info,
.footer .footer-note .text-end .footer_paypal,
.footer .footer-note .text-end .coppy-right{
    justify-content: flex-end;
}
.footer .footer-note .footer_paypal img{
    margin-right: 5px;
}
.footer .footer-note .footer_paypal img:last-child{
    margin-right: 0;
}
.footer .footer-note .right-content .social-footer-wrap{
	display: inline-flex;
    align-items: center;
}
.footer .footer-note .right-content .social-footer-wrap h3{
    line-height: 1;
    margin-right: 30px;
    color: #999999;
    font-size: 14px;
    margin: 0 15px 0 0;
    font-weight: 400;
}
.footer .footer-note .right-content .social-footer-wrap .laber-social-link .list-socials li a{
	color:#444444;
}
.footer .footer-note .right-content .social-footer-wrap .laber-social-link .list-socials li a:hover{
	color:#fff;
}
.footer .main-footer .content_html{
    margin: 0;
    padding: 0;
    font-size: 14px;
    position: relative;
    color: #999;
    display: flex;
    flex-direction: column;
}
.footer .main-footer .content_html p{
    line-height: 1;
    margin-bottom: 15px;
    padding: 0;
    font-size: 14px;
    position: relative;
    color: #999;
}
.footer .main-footer .footer_paypal{
    padding-top:10px;
}

.footer .footer-note.style_1 .footer-note-item.copyright{
    flex: 1;
}

.footer .footer-note.style_2 .footer-note-item{
    flex: 1;
}

@media (max-width: 991px) {
    .footer .footer-note .custom-html-link p:last-child{
        display:none;
    }
    .footer .footer-note .footer-note-item{
        order: 2;
    }
    .footer .footer-note .footer-note-item.copyright{
        order: 1;
        flex: 0 0 100%;
/*         margin-bottom: 20px; */
    }
    .footer .footer-note.style_2 .footer-note-item{
        flex: 0 0 100%;
    }
    .footer .footer-note.style_2 .text-start .footer_paypal, 
    .footer .footer-note.style_2 .text-start .coppy-right,
    .footer .footer-note.style_2 .text-end .footer_paypal, 
    .footer .footer-note.style_2 .text-end .coppy-right{
        justify-content: center;
        text-align: center;
    }
}
@media (max-width: 480px) {
    .footer .footer-note .left-content {
        text-align: center !important;
    }
    .footer .footer-note{padding: 30px 0;}
    .footer .main-footer{padding-bottom:30px;}
    .footer .footer-top{padding-top:30px;}
    .footer .footer-note .custom-html-link p{display:none;}
    .footer .footer-note .right-content {
        text-align: center !important;
    }
    .footer .footer-note .footer_paypal{
        margin-top:0;
    }
}
@media(max-width: 479px){
    .footer .footer-note .footer-note-item{
        flex: 0 0 100%;
        margin-bottom: 10px;
    }

    .footer .footer-note .text-start .footer_paypal, 
    .footer .footer-note .text-start .coppy-right,
    .footer .footer-note .text-start .inner-extra-info,
    .footer .footer-note .text-end .footer_paypal, 
    .footer .footer-note .text-end .coppy-right,
    .footer .footer-note .text-end .inner-extra-info{
        justify-content: center;
        text-align: center;
    }
}
.footer .footer-note #footer-menu {
    margin: 0;
    margin-left: -5px;
    }
.footer .footer-note #footer-menu li {
    list-style: none;
    margin-left: 34px;
    display: inline-block;
}
.footer .footer-note #footer-menu li:first-child {
    margin: 0px;
}
.footer .footer-note #footer-menu li a {
    color: #666;
    font-size: 14px;
    text-transform: capitalize;
    position: relative;
    letter-spacing: -0.3px;
    padding: 0px 5px;
}
.footer .footer-note #footer-menu li a:before {
    position: absolute;
    width: 1px;
    height: 14px;
    content: "";
    right: -17px;
    bottom: 3px;
    background: #666;
}
.footer .footer-note #footer-menu li a:after {
    position: absolute;
    content: "";
    background-color: #1A73E8;
    left: 0px;
    opacity: 0;
    width: 0%;
    height: 8px;
    bottom: 0%;
    -webkit-transition: all 0.3s linear 0ms;
    -khtml-transition: all 0.3s linear 0ms;
    -moz-transition: all 0.3s linear 0ms;
    -ms-transition: all 0.3slinear 0ms;
    -o-transition: all 0.3s linear 0ms;
    transition: all 0.3s linear 0ms;
}
.footer .footer-note #footer-menu li a:hover {
    color: #000;
}
.footer .footer-note #footer-menu li a:hover:after {
    opacity: 0.3;
    width: 100%;
}
@media (max-width: 480px) {
    .footer .footer-note #footer-menu li {
        margin: 0px;
        display: block;
    }
    .footer .footer-note #footer-menu li a:before {
        display: none;
    }
}
.footer .footer-note #footer-menu li:last-child a:before {
    display: none;
}
.footer .footer-note .coppy-right .content {
    font-size: 20px;
    line-height: 27px;
    margin: 0;
    font-weight: 500;
}
.footer .footer-note .coppy-right .content a {
    font-weight: 400;
}
@media (max-width: 375px) {
    .footer .footer-note .coppy-right .content {
        text-align:center;
        line-height:20px;
    }
}
.footer .footer-note .right-content {
    text-align: right;
    flex-grow: 1;
}

.back-to-top {
	bottom: 60px;
  font-weight: 700;
  font-size: 18px;
  position: fixed;
  right: 30px;
  color: var(--color-btn);
  background-color: var(--background-btn);
  width: 40px;
  height: 40px;
  z-index: 99;
  overflow: hidden;
  padding: 0;
  border-radius: var(--border-radius);
  align-items: center;
  display: flex;
  justify-content: center;
  opacity: 0;
  box-shadow: 1px 2px 7px 0px rgb(0 0 0 / 8%);
}

.switch-rtl-ltr span{
	bottom: 136px;
  font-weight: 500;
  font-size: 14px;
  position: fixed;
  right: 30px;
  color: var(--color-btn-text);
  width: 50px;
  height: 50px;
  z-index: 99;
  overflow: hidden;
  padding: 0;
  border-radius: var(--border-radius);
  background-color: var(--color-btn);
  align-items: center;
  line-height: 50px;
  text-align: center;
  justify-content: center;
  box-shadow: 1px 2px 7px 0px rgb(0 0 0 / 8%);
}

.switch-rtl-ltr span:hover{
	background-color: var(--color-btn-hover);
    color: var(--color-btn-text_hover);
}
.ltr-text{display:block;}
.ltr-text{display:none;}
.rtl .rtl-text{display:none;}
.rtl .switch-rtl-ltr span{
	left:30px;
    right:auto;
}
.rtl .ltr-text{
    display:block;
}
.back-to-top:hover,
.back-to-top:focus,
.back-to-top:active {
    background-color: var(--color-btn-hover);
    color: var(--color-btn-text_hover);
}
.back-to-top.show {
    opacity:1;
}
@media (max-width: 1199px) {
