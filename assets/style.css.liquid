  .title_sidebar h3{
    font-size: 13px;
    font-weight: 500;
    background: var(--color-main);
      margin: 0;
      height: 60px;
      line-height: 60px;
      color: #fff;
      padding: 0 15px;
      margin-bottom: 20px;
      text-transform: uppercase;
