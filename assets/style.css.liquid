{%- assign productionMode = settings.production_mode -%}
{%- capture styleCss -%}
{%- liquid
  assign body_font_bold = settings.type_body_font | font_modify: 'weight', 'bold'
  assign body_font_italic = settings.type_body_font | font_modify: 'style', 'italic'
  assign body_font_bold_italic = body_font_bold | font_modify: 'style', 'italic'
%}
{{ settings.type_body_font | font_face: font_display: 'swap' }}
{{ body_font_bold | font_face: font_display: 'swap' }}
{{ body_font_italic | font_face: font_display: 'swap' }}
{{ body_font_bold_italic | font_face: font_display: 'swap' }}
{{ settings.type_heading_font | font_face: font_display: 'swap' }}

:root {
  /* TYPOGRAPHY */
  {% if settings.select_body_font == 'shopify_font' %}
    --font-body-family: {{ settings.type_body_font.family }}, {{ settings.type_body_font.fallback_families }};
  {% else %}
    --font-body-family: {{ settings.font_family | remove: "font-family: " | remove: ";" }};
  {% endif %}
  {% if settings.select_heading_font == 'shopify_font' %}
    --font-heading-family: {{ settings.type_heading_font.family }}, {{ settings.type_heading_font.fallback_families }};
  {% else %}
    --font-heading-family: {{ settings.font_family_heading | remove: "font-family: " | remove: ";" }};
  {% endif %}
  --font-icon: "Fontawesome";
  --font-icon-custom: 'icomoon';
  --font-body-style: {{ settings.type_body_font.style }};
  --font-body-weight: {{ settings.type_body_font.weight }};
  --font-body-weight-bold: {{ settings.type_body_font.weight | plus: 300 | at_most: 1000 }};
  --border-radius: 4px;

  /* COLOR */
  --color-bg-body:{{ settings.color_body_bg }};
  --color-text-body:{{ settings.color_body_text }};
  --color-main: {{ settings.color_main }};
  --color-secondary: {{ settings.color_secondary }};
  --color-link: {{ settings.color_link }};
  --color-link-hover: {{ settings.color_link_hover }};
  --color-scrollbar: {{ settings.color_scrollbar }};  
  --color-border: {{ settings.color_border }};

  /* COLOR BUTTON */
  --color-btn: {{ settings.color_btn }};
  --background-btn: {{ settings.background_btn }};
  --border-btn: {{ settings.border_btn }};
  --color-btn-hover: {{ settings.color_btn_hover }};
  --background-btn-hover: {{ settings.background_btn_hover }};
  --border-btn-hover: {{ settings.border_btn_hover }};

  /* COLOR PRODUCT */
  --color-compare-price: {{ settings.color_compare_price }};
  --color-price: {{ settings.color_price }};
  --color-in-stock: {{ settings.color_label_in }};
  --color-out-stock: {{ settings.color_label_out }};

  --color-btn-product: {{ settings.color_btn_product }};
  --background-btn-product: {{ settings.background_btn_product }};
  --border-btn-product: {{ settings.border_btn_product }};
  --color-btn-product-hover: {{ settings.color_btn_product_hover }};
  --background-btn-product-hover: {{ settings.background_btn_product_hover }};
  --border-btn-product-hover: {{ settings.border_btn_product_hover }};

  /* COLOR OTHERS */
  --color-rating: {{ settings.color_rating }};
  --color-label-hot: {{ settings.color_label_hot }};
  --color-label-new: {{ settings.color_label_new }};
  --color-label-sale: {{ settings.color_label_sale }};
}

html ::-webkit-scrollbar {
  width: 5px;
}  
html ::-webkit-scrollbar-thumb {
  background: var(--color-scrollbar);
    border-radius: 10px;
    }
html ::-webkit-scrollbar-track {
