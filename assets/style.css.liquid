/* slick slider */
.slick-prev,
.slick-next{
  background-color: var(--background-btn);
  border-radius: var(--border-radius);
  border-radius: 100%;
}
 

.slick-prev:not([disabled]):hover,
.slick-next:not([disabled]):hover,
.slick-prev:hover,
.slick-next:hover{
  background-color: var(--background-btn-hover);
}

.contentTestimonial.slick-next:not([disabled]),
.contentTestimonial .slick-next {
    background-color: var(--background-btn-hover) !important;
}


.slick-prev:focus,
.slick-next:focus{
  background-color: var(--background-btn);
}

.slick-prev:before,
.slick-next:before{
  font-family: var(--font-icon);
  font-size: 40px;
  color: var(--color-btn);
  opacity: 1;
}

.slick-prev:hover:before,
.slick-next:hover:before{
  color: var(--color-btn-hover);
}

.slick-prev:before{
  content: "\f104";
}

.slick-next:before{
  content: "\f105";
}

.slick-prev.slick-disabled:before, 
.slick-next.slick-disabled:before{
  opacity: 1;
  cursor: not-allowed;
}

.products-slider .slick-prev,
.products-slider .slick-next{
  top: 35%;
}
.articles-slider .slick-prev,
.products-slider .slick-prev{
  left: -5px;
}
.articles-slider .slick-next,
.products-slider .slick-next{
  right: -5px;
}


.slick-dots{
  position: static;
  width: fit-content;
}

.slick-dots li{
  border: 2px solid transparent;
  margin:0 3px;
  border-radius: 100%;
  width: 15px;
  height: 15px;
  line-height: 15px;
  text-align: center;
  -webkit-transition: all 240ms linear 0s;
  -khtml-transition: all 240ms linear 0ms;
  -moz-transition: all 240ms linear 0s;
  -ms-transition: all 240ms linear 0s;
  -o-transition: all 240ms linear 0s;
  transition: all 240ms linear 0s;
}
.slick-dots li.slick-active{
  border-color:var(--color-main);
}
.slick-dots li button{
  width: 11px;
  height: 11px;
}
.slick-dots li button::before{
  width: 12px;
  height: 12px;
  line-height: 12px;
  font-size: 6px;
  opacity: 1;
  content: '\f111';
  font-family: FontAwesome;
  color: #e8e8e8;
}
.slick-dots li.slick-active button:before{
  opacity: 1;
  color: transparent;
}
